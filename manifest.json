{
  "name": "AniDB omnibox",
  "description": "AniDB omnibox search predictions",
  "version": "1.0",
  "omnibox": {
    "keyword": "ani"
  },
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "128": "icon/128.png",
    "256": "icon/256.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "alarms",
    "storage",
    "notifications"
  ],
  "host_permissions": [
    "https://anidb.net/",
    "https://cdn-eu.anidb.net/"
  ],
  "manifest_version": 3,
  "minimum_chrome_version": "102"
}
